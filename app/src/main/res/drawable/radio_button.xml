<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true" android:state_enabled="true">
        <layer-list>
            <item android:drawable="@drawable/ic_check_enabled" android:left="10dp" />
        </layer-list>
    </item>
    <item android:state_checked="false" android:state_enabled="true">
        <layer-list>
            <item android:drawable="@drawable/ic_uncheck_enabled" android:left="10dp" />
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item android:drawable="@drawable/ic_uncheck_enabled" android:left="10dp" />
        </layer-list>
    </item>
</selector>